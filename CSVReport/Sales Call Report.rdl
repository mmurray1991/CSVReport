  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS_Report</DataSourceName>
        <CommandText>select [From User],
       COUNT([Talking Time]) as Outgoing
from dbo.CALL_RECORDS_MASTER
Where [Talking Time] &gt;= 30
 AND  DATEDIFF(day,CONVERT(date,SUBSTRING([Call Time],2,LEN([Call Time])-2)),GETDATE()) &lt; 31
group by [From User]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="From_User">
          <DataField>From User</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Outgoing">
